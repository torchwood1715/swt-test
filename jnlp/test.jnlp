<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="7.0+"
    codebase="https://github.com/torchwood1715/swt-test"
    href="test.jnlp"
    version="1.0">

  <information>
    <title>SWT Test</title>
  </information>
  <security>
     <all-permissions/>
  </security>

  <resources>
    <j2se version="17"/>
    <jar href="lib/SWTmacOSTest-1.0-SNAPSHOT.jar"/>
  </resources>

  <resources os="Linux" arch="x86_64">
    <jar href="lib/org.eclipse.swt.gtk.linux.x86_64-3.117.0.jar"/>
  </resources>

  <resources os="Windows">
    <jar href="lib/org.eclipse.swt.win32.win32.x86_64-3.117.0.jar"/>
  </resources>

  <resources os="Mac" arch="x86_64">
    <jar href="lib/org.eclipse.swt.cocoa.macosx.x86_64-3.117.0.jar"/>
    <j2se version="17" java-vm-args="-XstartOnFirstThread"/>
  </resources>

  <application-desc main-class="org.eclipse.swttest.Main" name="SWT Test" />
  <update check="always" policy="always" />
</jnlp>