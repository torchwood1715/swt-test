<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="7.0+"
      codebase="https://github.com/torchwood1715/swt-test/blob/main/jnlp"
      href="test.jnlp?raw=true"
      version="1.0">

  <information>
    <title>SWT Test</title>
  </information>
  <security>
     <all-permissions/>
  </security>

  <resources>
    <jar href="lib/SWTmacOSTest-1.0-SNAPSHOT.jar?raw=true" main="true"/>
  </resources>

  <resources os="Linux" arch="x86_64">
    <j2se version="11"/>
    <jar href="lib/org.eclipse.swt.gtk.linux.x86_64-3.117.0.jar?raw=true"/>
  </resources>

  <resources os="Windows">
    <j2se version="11"/>
    <jar href="lib/org.eclipse.swt.win32.win32.x86_64-3.117.0.jar?raw=true"/>
  </resources>

  <resources os="Mac" arch="x86_64">
    <jar href="lib/org.eclipse.swt.cocoa.macosx.x86_64-3.117.0.jar?raw=true"/>
    <j2se version="11" java-vm-args="-XstartOnFirstThread"/>
  </resources>

  <application-desc main-class="org.eclipse.swttest.Main" name="SWT Test?raw=true" />
  <update check="always" policy="always" />
</jnlp>
